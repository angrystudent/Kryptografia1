#include <stdio.h>
#include <time.h>

int getPrime(int bits);
 
int main(void)
{
    int  prime;
    prime = getPrime(256);
    printf("Liczba pierwsza %d\n", prime);
 
	return 1;
}

int getPrime(int bits)
{
    int n;
    while(1)
    {
        int x;
        srand((unsigned) time(NULL));
        x = rand();
        n = rand();
 
        if(x > 2 && x < n - 2)
        {
            int rest;
            rest = pow(x, n - 1);
            rest = r % n;
            if(r = 1)break;
        }
    }
    return n;
}
